<materials>
    <material name="water512.jpg">
      <shader type="depthwrite">*null</shader>
      <shader type="base">reflect_water_plane</shader>
      <shader type="diffuse">reflect_water_plane</shader>
      <shadervar type="texture" name="tex normal">water_n</shadervar>
      <!--<shadervar type="texture" name="tex environment">skyBox</shadervar>-->
      <shadervar type="vector4" name="water fog color">0,0.1,.15,1</shadervar>
      <shadervar type="float" name="water fog density">3</shadervar>
      <shadervar type="vector4" name="specular">1,1,1,1</shadervar>
      <shadervar name="water perturb scale" type="vector4">0.9,0.9,0,0</shadervar>
